{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "browsers": [">0.25%", "not dead"],
        "node": "current"
      },
      "modules": false
    }],
    ["@babel/preset-react", {
      "runtime": "automatic"
    }]
  ],
  "sourceType": "module",
  "plugins": [
    "@babel/plugin-transform-runtime"
  ]
}
